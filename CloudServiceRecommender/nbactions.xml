<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>build-with-dependencies</actionName>
            <reactor>also-make</reactor>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>install</goal>
                <goal>-U</goal>
                
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Clean Package Run</actionName>
            <displayName>Clean Package Run</displayName>
            <goals>
                <goal>clean</goal>
                <goal>package</goal>
            </goals>
            <properties>
                <netbeans.deploy>true</netbeans.deploy>
            </properties>
        </action>
    </actions>
